{
  "name": "Blegg",
  "version": "1.0.0",
  "description": "Blog platform for Coursework 2 of SET008101",
  "main": "index.js",
  "repository": "https://github.com/mattdelsordo/delsordo_matthew_set008101_coursework2.git",
  "author": "Matt DelSordo <mattdelsordo@gmail.com>",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "npm install && npm run-script prod:build && npm run-script prod:start",
    "stop": "npm run-script prod:stop",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "npm run-script lint && flow && jest --coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-flow-react-proptypes": "^21.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.67.1",
    "jest": "^22.4.3",
    "nodemon": "^1.17.1",
    "pm2": "^2.10.1",
    "rimraf": "^2.6.2",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "^0.0.17",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0-alpha.6",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "immutable": "4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.0.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-thunk": "^2.2.0",
    "tether": "^1.4.3",
    "webpack-cli": "^2.0.11"
  }
